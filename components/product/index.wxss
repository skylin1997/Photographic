.container{
  display: flex;
  flex-direction: column;
  align-items: center;
  width:196rpx;
  height: 216rpx;
  background: #fff;
  border:1px solid rgba(209,209,209,1);
  border-radius: 14rpx;
  box-shadow: 0 4rpx 8rpx 0 rgba(0, 0, 0, 0.16);
   margin:20rpx 0 20rpx 0;
}
.container image{
  width:162rpx;
  height:162rpx;
  margin-top:14rpx;
}
.msg{
  width:100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: relative;
  top:8rpx;
  font-size: 24rpx;
}
.msg-price{
  font-family: Microsoft YaHei;
  font-weight: Bold;
  color: rgba(251,22,66,1);
}